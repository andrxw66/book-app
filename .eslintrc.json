{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": "eslint:recommended",
	"overrides": [
	],
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"rules": {
		"quotes": [
			2,
			"single",
			{
				"avoidEscape": true
			}
		],
		"semi": ["error", "never"],
		"no-multi-spaces": ["error"]
	}
}